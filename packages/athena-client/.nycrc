{
  "extension": [
    ".ts",
    ".tsx"
  ],
  "description": "Code coverage generation setting",
  "check-coverage": true,
  "per-file": true,
  "lines": 70,
  "statements": 70,
  "functions": 100,
  "branches": 0,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/index.ts"
  ],
  "ignore-class-method": "methodToIgnore",
  "reporter": [
    "text",
    "html"
  ],
  "cache": true,
  "all": true,
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage"
}
