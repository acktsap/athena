[
  {
    "prefix": "--[[",
    "snippet": "--[[ ${1:comment...} ]]"
  },
  {
    "prefix": "local",
    "snippet": "local ${1:var} = ${2:init}"
  },
  {
    "prefix": "function",
    "snippet": "function ${1:funcName}(${2:args...})\n\t${3:-- body...}\nend"
  },
  {
    "prefix": "constructor",
    "snippet": "function constructor(${1:args...})\n\t${2:-- body...}\nend"
  },
  {
    "prefix": "default",
    "snippet": "function default(${1:args...})\n\t${2:-- body...}\nend"
  },
  {
    "prefix": "while",
    "snippet": "while ${1:condition} do\n\t${2:-- body...}\nend"
  },
  {
    "prefix": "forpairs",
    "snippet": "for ${1:key},${2:value} in pairs(${3:table_name}) do\n\t${4:-- body...}\nend"
  },
  {
    "prefix": "foripairs",
    "snippet": "for ${1:index},${2:value} in ipairs(${3:table_name}) do\n\t${4:-- body...}\nend"
  },
  {
    "prefix": "if",
    "snippet": "if ${1:value} then\n\t${2:--body...}\nend"
  },
  {
    "prefix": "ifelse",
    "snippet": "if ${1:value} then\n\t${2:--body...}\nelse\n\t${3:--body...}\nend"
  }
]